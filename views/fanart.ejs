<body>
  <h1 style="font-family: double feature; color: rgb(245, 88, 16); font-size: 75px; text-align: center;">FAN ART</h1>
  <a href="/gpk/gallery/add" class="add-button" style="margin: 20px;">ADD ART</a>
  <div class="container">
      <% artists.forEach((artist) => { %>
          <div class="flip-card">
              <div class="card-inner">
                  <div class="card-front">
                      <img style="height: 400px; width: 270px; border: solid rgba(170, 173, 137) 1px;border-left: solid rgb(131, 133, 109) 1px;border-radius: 5px;" src="<%= artist.img %>">
                    </div>
                    <div class="card-back">
                        <h2 style="background-color: antiquewhite; margin-top: -50px;"><%= artist.artist.toUpperCase() %></h2>
                      <ul class="card-info">
                          <li>Card Name: <%= artist.cardName %></li>
                          <li>Price: $<%= artist.price %></li>
                          <li>Website:</li><br>
                          <a href="<%= artist.website %>" style="background-color: rgb(207, 3, 163); width: 100px; padding: 3px; height: 20px; color: rgb(207, 169, 169); border: 1px solid white; text-align: center;">CLICK ME</a>
                      </ul>
                  </div>
              </div>
          </div>
          <% }) %>  

      </div>
  </body>