<body style="background-color: rgb(131, 160, 4); font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;">
    <h1 style="color:rgb(215, 241, 176); text-align: left; margin-left: 15px; text-transform: uppercase;"><%= cards[0].series %></h1>
    <% const addButton = cards[0].series.toLowerCase().split(" ").join("") %>
    <button><a href="/gpk/gallery/add">ADD ART</a></button>
    <div class="container" style="display: flex; width: 875px; flex-wrap: wrap; justify-content: left; float: left;">
                <% artists.forEach((artist) => { %>
                  <div class="fanArt" style="margin: 10px; background-color: rgb(215, 241, 176); padding: 5px; padding-bottom: 10px; border: solid green 2px; border-radius: 4px;">
                    <img style="height: 400px; width: 250px; border: solid rgb(228, 250, 26) 2px; border-radius: 3px;" src="<%= artist.img %>">
                  </div>
                  <div>
                    <h1><%= artist.artist %></h1>
                    <ul style="text-align: center; width: 200px">
                        <li style="list-style-type: none;"><%= artist.cardName %></li>
                        <li style="list-style-type: none;"><%= artist.price %></li>
                        <li style="list-style-type: none;"><%= artist.website %></li>
                    </ul>
                </div>
        <% }) %>
    </div>
</body>
</html>